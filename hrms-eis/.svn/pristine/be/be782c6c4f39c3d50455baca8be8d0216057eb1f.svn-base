		this.allowanceVal = allowanceVal;
	}
	/**
	 * @return the allowDesc
	 */
	public String getAllowDesc() {
		return allowDesc;
	}
	/**
	 * @param allowDesc the allowDesc to set
	 */
	public void setAllowDesc(String allowDesc) {
		this.allowDesc = allowDesc;
	}
	

}
