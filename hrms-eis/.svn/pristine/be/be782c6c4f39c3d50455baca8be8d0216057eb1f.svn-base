	private String allowDesc;
	private long allowanceVal;
	private long allwDedId;
	public long getAllwDedId()
	{
		return allwDedId;
	}
	public void setAllwDedId(long allwDedId)
	{
		this.allwDedId = allwDedId;
	}
	public long getAllwID() {
		return allwID;
	}
	public void setAllwID(long allwID) {
		this.allwID = allwID;
	}
	public long getAllowanceVal() {
		return allowanceVal;
	}
