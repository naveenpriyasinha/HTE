<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tcs.sgv.eis.valueobject.HrPaySignatureDtls" table="HR_PAY_SIGNATURE_DTLS">
        <id name="id" type="long">
            <column name="id"  />
            <generator class="assigned" />
        </id>        
          <many-to-one name="hrEisEmpMst" class="com.tcs.sgv.eis.valueobject.HrEisEmpMst" fetch="select">
            <column name="EMP_ID"  >
                <comment></comment>
            </column>
        </many-to-one>
        <many-to-one name="orgDesignationMst" class="com.tcs.sgv.ess.valueobject.OrgDesignationMst" fetch="select">
            <column name="DSGN_ID" >
                <comment></comment>
            </column>
        </many-to-one>
        <property name="startDate" type="date">
            <column name="START_DATE" length="7" />
        </property>
        <property name="endDate" type="date">
            <column name="END_DATE" length="7" />
        </property>
        <many-to-one name="cmnLocationMst" class="com.tcs.sgv.common.valueobject.CmnLocationMst" fetch="select">
            <column name="loc_id" >
                <comment></comment>
            </column>
        </many-to-one>
        <many-to-one name="orgPostMstByUpdatedByPost" class="com.tcs.sgv.ess.valueobject.OrgPostMst" fetch="select">
            <column name="updated_by_post">
                <comment></comment>
            </column>
        </many-to-one>
        <many-to-one name="orgPostMstByCreatedByPost" class="com.tcs.sgv.ess.valueobject.OrgPostMst" fetch="select">
            <column name="created_by_post" >
                <comment></comment>
            </column>
        </many-to-one>
        <many-to-one name="orgUserMstByUpdatedBy" class="com.tcs.sgv.ess.valueobject.OrgUserMst" fetch="select">
            <column name="updated_by">
                <comment></comment>
            </column>
        </many-to-one>
        <many-to-one name="orgUserMstByCreatedBy" class="com.tcs.sgv.ess.valueobject.OrgUserMst" fetch="select">
            <column name="created_by" >
                <comment></comment>
            </column>
        </many-to-one>  
        <property name="createdDate" type="date">
            <column name="CREATED_DATE" length="7" />
        </property>
        <property name="updatedDate" type="date">
            <column name="UPDATED_DATE" length="7" />
        </property>              
        <property name="empFullname" type="string">
            <column name="emp_fullname"  />
        </property>
   </class>
</hibernate-mapping>
