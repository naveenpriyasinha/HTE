package com.tcs.sgv.eis.valueobject;
/**
 * This Class is for Mapping the Non-Government Details.
 * @author Urvin shah
 * Date 4-Oct-2008
 */
public class NonGovCustomVO {
	private String empName;
	private long deducAmount;
	private String accNumber;
	/**
	 *  Default Constructor
	 */
	public NonGovCustomVO() {
		super();
	}
	/**
	 * @param empName
	 * @param deducAmount
	 * @param accNumber
	 */
	public NonGovCustomVO(String empName, long deducAmount, String accNumber) {
		super();
		this.empName = empName;
		this.deducAmount = deducAmount;
		this.accNumber = accNumber;
	}
	public String getAccNumber() {
		return accNumber;
	}
	public void setAccNumber(String accNumber) {
		this.accNumber = accNumber;
	}
	public long getDeducAmount() {
		return deducAmount;
	}
	public void setDeducAmount(long deducAmount) {
		this.deducAmount = deducAmount;
	}
	public String getEmpName() {
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
	}
}
