<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.tcs.sgv.eis.valueobject.HrPayPaycommissionMst"
		table="HR_PAY_PAYCOMMISSION_MST">
		<id name="id" type="long">
			<column name="ID" precision="10" scale="0" not-null="true" />
			<generator class="assigned" />
		</id>

		<property name="commissionName" type="string">
			<column name="COMMISSION_NAME" not-null="true" />
		</property>
		<property name="commissionShortName" type="string">
			<column name="COMMISSION_SHORT_NAME" />
		</property>

		<property name="startDate" type="date">
			<column name="START_DATE" length="7" not-null="true" />
		</property>
		<property name="endDate" type="date">
			<column name="END_DATE" length="7" not-null="true" />
		</property>

		<property name="commissionCode" type="string">
			<column name="COMMISSION_CODE" not-null="true" />
		</property>

		
		<many-to-one name="orgUserMstBycrtdBy" class="com.tcs.sgv.ess.valueobject.OrgUserMst"
			fetch="select">
			<column name="CREATED_BY" precision="20" scale="0" not-null="true">
				<comment></comment>
			</column>
		</many-to-one>

		<many-to-one name="orgPostMstBycrtdByPost" class="com.tcs.sgv.ess.valueobject.OrgPostMst"
			fetch="select">
			<column name="CREATED_BY_POST" precision="20" scale="0"
				not-null="true">
				<comment></comment>
			</column>
		</many-to-one>

		<property name="crtdDate" type="date">
			<column name="CREATED_DATE" length="7" not-null="true" />
		</property>

		<many-to-one name="orgUserMstUpdBy" class="com.tcs.sgv.ess.valueobject.OrgUserMst"
			fetch="select">
			<column name="UPDATED_BY" precision="20" scale="0" not-null="false">
				<comment></comment>
			</column>
		</many-to-one>

		<many-to-one name="orgPostMstUpdByPost" class="com.tcs.sgv.ess.valueobject.OrgPostMst"
			fetch="select">
			<column name="UPDATED_BY_POST" precision="20" scale="0"
				not-null="false">
				<comment></comment>
			</column>
		</many-to-one>

		<property name="updDate" type="date">
			<column name="UPDATED_DATE" length="7" />
		</property>
		<property name="langId" type="long">
			<column name="LANG_ID" length="20" />
		</property>

	</class>
</hibernate-mapping>