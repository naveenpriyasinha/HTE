package com.tcs.sgv.hr.payincrement.valueobject;

import java.util.Date;

public class EmpPayIncrVOCus {
	
	long PayFixId;
	long ScaleId;
	String EmpName;
	String CurrentPayScale;
	long CurrentPay;
	long revPay;
	String ASE;
	long LWP;
	Date PayFixDate;
	Date NextIncrDate;
	long UserId;
	char DeferedFlag;
	String Remarks;
	Date LastIncrDate;
	Date EffectiveDate;
	
	public EmpPayIncrVOCus(){
		super();
	}
	
	/**
	 * @param payFixId
	 * @param scaleId
	 * @param empName
	 * @param currentPayScale
	 * @param currentPay
	 * @param revPay
	 * @param ase
	 * @param lwp
	 * @param payFixDate
	 * @param nextIncrDate
	 * @param userId
	 * @param deferedFlag
	 * @param remarks
	 * @param lastIncrDate
	 * @param effectiveDate
	 */
	public EmpPayIncrVOCus(long payFixId, long scaleId, String empName, String currentPayScale, long currentPay, long revPay, String ase, long lwp, Date payFixDate, Date nextIncrDate, long userId, char deferedFlag, String remarks, Date lastIncrDate, Date effectiveDate) {
		super();
		this.PayFixId = payFixId;
		this.ScaleId = scaleId;
		this.EmpName = empName;
		this.CurrentPayScale = currentPayScale;
		this.CurrentPay = currentPay;
		this.revPay = revPay;
		this.ASE = ase;
		this.LWP = lwp;
		this.PayFixDate = payFixDate;
		this.NextIncrDate = nextIncrDate;
		this.UserId = userId;
		this.DeferedFlag = deferedFlag;
		this.Remarks = remarks;
		this.LastIncrDate = lastIncrDate;
		this.EffectiveDate = effectiveDate;
	}
	public Date getEffectiveDate() {
		return EffectiveDate;
	}
	public void setEffectiveDate(Date effectiveDate) {
		EffectiveDate = effectiveDate;
	}
	public Date getLastIncrDate() {
		return LastIncrDate;
	}
	public void setLastIncrDate(Date lastIncrDate) {
		LastIncrDate = lastIncrDate;
	}
	public String getRemarks() {
		return Remarks;
	}
	public void setRemarks(String remarks) {
		Remarks = remarks;
	}
	public char getDeferedFlag() {
		return DeferedFlag;
	}
	public void setDeferedFlag(char deferedFlag) {
		DeferedFlag = deferedFlag;
	}
	public long getUserId() {
		return UserId;
	}
	public void setUserId(long userId) {
		UserId = userId;
	}
	public String getASE() {
		return ASE;
	}
	public void setASE(String ase) {
		ASE = ase;
	}
	public long getCurrentPay() {
		return CurrentPay;
	}
	public void setCurrentPay(long currentPay) {
		CurrentPay = currentPay;
	}
	public String getCurrentPayScale() {
		return CurrentPayScale;
	}
	public void setCurrentPayScale(String currentPayScale) {
		CurrentPayScale = currentPayScale;
	}
	public String getEmpName() {
		return EmpName;
	}
	public void setEmpName(String empName) {
		EmpName = empName;
	}
	public long getLWP() {
		return LWP;
	}
	public void setLWP(long lwp) {
		LWP = lwp;
	}
	public Date getNextIncrDate() {
		return NextIncrDate;
	}
	public void setNextIncrDate(Date nextIncrDate) {
		NextIncrDate = nextIncrDate;
	}
	public Date getPayFixDate() {
		return PayFixDate;
	}
	public void setPayFixDate(Date payFixDate) {
		PayFixDate = payFixDate;
	}
	public long getPayFixId() {
		return PayFixId;
	}
	public void setPayFixId(long payFixId) {
		PayFixId = payFixId;
	}
	public long getScaleId() {
		return ScaleId;
	}
	public void setScaleId(long scaleId) {
		ScaleId = scaleId;
	}
	public long getRevPay() {
		return revPay;
	}
	public void setRevPay(long revPay) {
		this.revPay = revPay;
	}
	
}
