	<sql-query name="pension.billDtls.pnsnrDtlsFrmBill">
		<![CDATA[
			SELECT D.PENSIONER_CODE,D.FROM_DATE
          	FROM TRN_PENSION_BILL_DTLS D, TRN_PENSION_BILL_HDR H
         	WHERE D.TRN_PENSION_BILL_HDR_ID = H.TRN_PENSION_BILL_HDR_ID  
         	AND H.BILL_TYPE IN ('PENSION','Monthly') 
         	AND D.PAY_FOR_MONTH = H.FOR_MONTH AND H.BILL_NO IN (:BILL_LIST)
		]]>
	</sql-query>
	<sql-query name="pension.billDtls.pnsnrRedPnsn">
		<![CDATA[
			SELECT D.PENSIONER_CODE,D.REDUCED_PENSION,D.PAY_FOR_MONTH,BR.BILL_NO,BR.SUBJECT_ID,BR.CURR_BILL_STATUS
			FROM TRN_PENSION_BILL_DTLS D JOIN TRN_PENSION_BILL_HDR H ON D.TRN_PENSION_BILL_HDR_ID=H.TRN_PENSION_BILL_HDR_ID AND H.REJECTED != 1 AND H.FOR_MONTH = D.PAY_FOR_MONTH
			JOIN TRN_BILL_REGISTER BR ON BR.BILL_NO = H.BILL_NO AND D.PENSIONER_CODE IN (:PENSIONER_CODE_LIST) AND ((BR.SUBJECT_ID = 9 AND BR.PPO_NO IS NOT NULL) OR BR.SUBJECT_ID = 44) AND ((BR.CURR_BILL_STATUS BETWEEN 90 AND 160) OR BR.CURR_BILL_STATUS = -1)
			ORDER BY PENSIONER_CODE,PAY_FOR_MONTH DESC
		]]>
	</sql-query>
