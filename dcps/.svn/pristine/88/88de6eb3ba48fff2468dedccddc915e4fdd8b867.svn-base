<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tcs.sgv.dcps.valueobject.NoticeBoard" table="MST_DCPS_NOTICE_BOARD">

		<id name="dcpsNoticeIdPk" type="long">
			<column name="DCPS_NOTICE_ID" precision="20" scale="0" />
			<generator class="assigned" />
		</id>

		<property name="noticeSubject" type="string">
			<column name="NOTICE_SUBJECT" />
		</property>
		<property name="issuedBy" type="string">
			<column name="ISSUED_BY" />
		</property>
		<property name="issuedDate" type="timestamp">
			<column name="ISSUED_DATE" />
		</property>
		<property name="expiryDate" type="timestamp">
			<column name="EXPIRY_DATE" />
		</property>
		<property name="noticeType" type="string">
			<column name="NOTICE_TYPE" />
		</property>
		<property name="fieldDeptCode" type="string">
			<column name="FIELD_DEPT_CODE" />
		</property>
		<property name="fileName" type="string">
			<column name="FILE_NAME" />
		</property>
		<property name="PostId" type="long">
			<column name="CREATED_POST_ID" />
		</property>
		<property name="UserId" type="long">
			<column name="CREATED_USER_ID" />
		</property>
		<property name="CreatedDate" type="timestamp">
			<column name="CREATED_DATE" />
		</property>

		<property name="UpdatedPostId" type="long">
			<column name="UPDATED_POST_ID" />
		</property>
		<property name="UpdatedUserId" type="long">
			<column name="UPDATED_USER_ID" />
		</property>
		<property name="UpdatedDate" type="timestamp">
			<column name="UPDATED_DATE" />
		</property>

	</class>
</hibernate-mapping>
