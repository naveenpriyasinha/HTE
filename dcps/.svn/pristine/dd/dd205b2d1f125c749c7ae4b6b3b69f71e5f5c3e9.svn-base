<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tcs.sgv.pensionpay.valueobject">
	<class name="com.tcs.sgv.pensionpay.valueobject.HstMstPensionerDtls"
		table="HST_MST_PENSIONER_DTLS">
		<composite-id name="id"
			class="com.tcs.sgv.pensionpay.valueobject.HstMstPensionerDtlsId">
			<key-property name="pensionerDtlsId" type="long">
				<column name="pensioner_dtls_id" precision="20" scale="0" />
			</key-property>
			<key-property name="trnCounter" type="integer">
				<column name="TRN_COUNTER" precision="11" scale="0" />
			</key-property>
		</composite-id>
		<property name="pensionerCode" type="string">
			<column name="PENSIONER_CODE" length="20" />
		</property>
		<property name="registrationNo" type="string">
			<column name="registration_no" />
		</property>
		<property name="bankCode" type="string">
			<column name="BANK_CODE" length="20" />
		</property>
		<property name="branchCode" type="string">
			<column name="BRANCH_CODE" length="20" />
		</property>
		<property name="accountNo" type="string">
			<column name="ACOUNT_NO" length="20" />
		</property>
		<property name="locationCode" type="string">
			<column name="LOCATION_CODE" length="10" />
		</property>
		<property name="activeFlag" type="string">
			<column name="ACTIVE_FLAG" length="1" />
		</property>
		<property name="createdUserId" type="big_decimal">
			<column name="CREATED_USER_ID" precision="20" scale="0" />
		</property>
		<property name="createdPostId" type="big_decimal">
			<column name="CREATED_POST_ID" precision="20" scale="0" />
		</property>
		<property name="createdDate" type="timestamp">
			<column name="CREATED_DATE" length="7" />
		</property>
		<property name="updatedUserId" type="big_decimal">
			<column name="UPDATED_USER_ID" precision="20" scale="0" />
		</property>
		<property name="updatedPostId" type="big_decimal">
			<column name="UPDATED_POST_ID" precision="20" scale="0" />
		</property>
		<property name="updatedDate" type="timestamp">
			<column name="UPDATED_DATE" length="7" />
		</property>
		<property name="oldStateCode" type="string">
			<column name="OLD_STATE_CODE" length="10" />
		</property>
		<property name="oldTreasury" type="string">
			<column name="OLD_TREASURY" length="30" />
		</property>
		<property name="oldSubTreasury" type="string">
			<column name="OLD_SUB_TREASURY" length="30" />
		</property>
		<property name="caseStatus" type="string">
			<column name="CASE_STATUS" length="10" />
		</property>
		<property name="bankEmailId" type="string">
			<column name="BANK_EMAIL_ID" length="30" />
		</property>
		<property name="isPwrofAtrny" type="string">
			<column name="IS_PWROF_ATRNY" length="5" />
		</property>
		<property name="identificationFlag" type="string">
			<column name="IDENTIFICATION_FLAG" />
		</property>
		<property name="identificationDate" type="timestamp">
			<column name="IDENTIFICATION_DATE" />
		</property>
	</class>
</hibernate-mapping>