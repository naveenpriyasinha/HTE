hrms_html {
	padding: 0;
	margin: 0px;
	/*background-color: rgb(228, 239, 243);*/
/*	background-color: rgb(228, 239, 248);*/
	text-align: left;
	font-family: Arial, Verdana, Helvetica, sans;
}

hrms_body {
	width: 99%;
	margin: 3px;
	/*background: url(header.gif) no-repeat;*/
	padding: 0 0 0 0;
	border: solid 1px #DEDEDE;
}

hrms_h1 {
	font-size: 14px;
	font-weight: normal;
	color: #90A720;
	margin: 0 0 0 0;
}

hrms_h2 {
	font-size: 14px;
	font-weight: bold;
	color: gray;
	margin: 0 0 0 0;
}

hrms_h3 {
	font-size: 13px;
	font-style: italic;
	color: #90A720;
	margin: 0 0;
}

hrms_h4 {
	font-size: 13px;
	font-style: italic;
	color: gray;
	margin: 0 0;
}

hrms_P {
	margin: 2px 0 0 0;
	padding: 0;
}

hrms_img {
	border: 0;
}

