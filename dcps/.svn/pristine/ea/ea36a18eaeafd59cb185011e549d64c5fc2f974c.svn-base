.hrms_headertable
{
	cellspacing: 0;
	cellpadding: 0;
	border: 0;	
	
}

.hrms_uheadertable_tr
{
	background: #336699;
	
}
	
.hrms_uheadertable_td
{
	padding: 3px;
	border-top: solid 1 #ffffff; 
	border-bottom: outset 1px #888888;
	
}
.hrms_uheadertable_td1
{
	padding: 3px; 
	border-top: solid 1 #ffffff; 
	border-bottom: outset 1px #888888;
	color: white;
}
.hrms_lheadertable_tr
{
		background: #777777;
		height: 4;
		
}
.hrms_lheadertable_td1
{
	padding: 0 3px 0 3px;
	border-top: solid 1px #999999; 
	border-bottom: outset 1px #666666;
	
	color: #FFFFFF;
	font-size: 15px;
	font-style: normal;
	font-weight: bold; 

}
.hrms_lheadertable_td2
{
	padding: 0 3px 0 3px;
	border-right: outset 1px #666666;
	border-top: solid 1px #999999; 
	border-bottom: outset 1px #666666; 
	color: white;
	
}
.hrms_lheadertable_td3
{
	padding: 0 10px 0 10px;
	border-left: solid 1px #999999;
	border-right: outset 1px #666666;
	border-top: solid 1px #999999; 
	border-bottom: outset 1px #666666; 
	color: white;
}
.hrms_lheadertable_td4
{
	padding: 0 10px 0 10px;
	border-left: solid 1px #999999; 
	border-top: solid 1px #999999; 
	border-bottom: outset 1px #666666; 
	color: white;	
}


.hrms_datatableheader {
	border: solid 1px gray;
	font-size: 11px;
	font-style: normal;
	font-weight: bold;
	text-align: center;
	background: #C9DFFF;
}

.hrms_tablecellnumber {
	border-bottom: solid 1px #DEDEDE;
	border-right: dotted 1px #DEDEDE;
	font-size: 10px;
	font-style: normal;
	font-weight: normal;
	text-align: right;
}

.hrms_tablecelltext {
	border-bottom: solid 1px #DEDEDE;
	border-right: dotted 1px #DEDEDE;
	font-size: 11px;
	font-style: normal;
	font-weight: normal;
	text-align: left;
}
