.hrms_groupTable{
	border: solid 1px gray;
	width: 100%;
	height: 100%;
	margin: 0px;
	cellpadding: 2px;
	border-collapse: collapse;
}

.hrms_groupTable td{
	padding: 2px;
/*	border: solid 1px #DEDEDE;*/
}

.hrms_tabtable{
/*	border-right: solid 1px #ffffff;
	border-bottom: solid 1px #ffffff;
	border-top: solid 1px #6587a9;
	border-left: solid 1px #6587a9;*/
	cellspacing: 0;
	width: 100%;
	background: trasnparant;
}
	
.hrms_tabtable td{
	padding: 3px;
}

.hrms_mandatory{
	background: #FFFFE9;
}

.hrms_button{
		border-top: solid 1px #FFFFFF;
	border-left: solid 1px #FFFFFF;
	border-right: solid 1px gray;
	border-bottom: solid 1px gray;
	background: #EFEFEF;
	width: 80px;
	
	}
