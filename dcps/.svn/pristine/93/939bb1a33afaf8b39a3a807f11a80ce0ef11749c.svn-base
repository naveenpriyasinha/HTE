.searchbutton{
	padding: 2px 5px 2px 2px;
	background: #efefef url(images/search-02.gif) no-repeat 5% center;
	text-align: right;
}
.captionTag{
	font-family: Verdana;
	text-decoration: none;
	font-size: 12px;
	font-weight: normal;
	color: Black;
}
.buttontag,.searchbutton{
	border: solid 1px #999999;
	background: #ffffff;
}
.buttontag:hover{
	border: solid 1px #999999;
	background: #ffffff;
}

.fieldGroupTag{
	position: relative;
	background:  #eeeeef url(images/fieldsettop.png) repeat-x top;
    border: 0px groove #aaa;
    border-bottom: 1px ridge #fff;
    border-left: 0px groove #aaa;
    border-right: 0px ridge #fff;
    padding: 2px 2px 2px 2px;
}
.fieldGroupExpand{
	position: absolute;
	background-color : transparent;
	background: url(images/taglib/fieldGrpMax.png) no-repeat;
	width: 16px;
	height: 14px;
}
.fieldGroupCollapse{
	position: absolute;
	background-color : transparent;
	background: url(images/taglib/fieldGrpMin.png) no-repeat;
	width: 16px;
	height: 14px;
}
.fieldGroupTag legend{
    border: 0px solid #678;
	font-size: 12px;
    font-weight:  bold;
    position: relative;
    top: 0px;
    left: -5px;
    width: 95%;
    padding: 2px 2px 2px 2px;
    color: black;
    margin-bottom: 3px;
}

.zoom_div {
	display: none;
	position: absolute;
	top: 20%;
	left: 25%;
	width: 50%;
	height: 400;
	padding: 1px;
	/*border: 4px solid orange;*/
	border: solid 1px #aaaaaa;
	background-color: #888888;
	z-index:1002;
}

.zoom_div table{
	width: 100%;
	height: 100%;
	padding: 1px;
	border: solid 1px #333333;
	background: #000000;
}
		
.zoom_div table table{
	border-left: solid 1px #efefef;
	border-top: solid 1px #efefef;
	border-right: solid 1px #898989;
	border-bottom: solid 1px #898989;
	background: #dddddd;
}

.imgtagtable{
	border: 1px solid #88aacc;
	background: #ccddff;
}

