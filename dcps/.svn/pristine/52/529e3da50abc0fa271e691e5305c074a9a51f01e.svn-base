}

#content {
	padding: 5px 5px 5px 5px;
	float: left; /*changed from none to left : shripal */
	z-index: 1;
	margin: 0px 0px 0px 0px;
	width: 99%;
	min-height: 450px;
	background: #FFFFFF;
	/*background: url(images/background.gif);*/
	font-size: 12px;
	color: #000000;
	text-align: left;
	border-left: solid 1px #ABABAB;
}

#footer {
	width: 100%;
	margin: 0;
	padding: 0 0 -5px 0;
	height: 100%;
	border-top: solid 1px #444444;
}

#footer ul {
	display: none;
}

#footertext {
	font-size: 11px;
	font-style: normal;
	font-weight: normal;
	color: #363636;
	text-align: right;
	width: 100%;
	border-top: solid 1px white;
}

.fieldLabel {
	font-family: Verdana;
	text-decoration: none;
	font-size: 12px;
	font-weight: normal;
	color: Black;
}

.screenLabel {
	font-family: Verdana;
	text-decoration: none;
	font-size: 12px;
	font-weight: normal;
	color: Black;
}

.groupHeaderLabel {
	background: #EEEEEE;
	/*	background: f0f7ff;*/
}

.groupHeaderLabel td {
	font-size: 13px;
	font-weight: bold;
	border: solid 1px #c0c0c0;
	padding: 0px 2px 0px 2px;
}

