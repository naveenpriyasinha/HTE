/* Added by Keyur For Long Size Buttons */
.bigbutton {
	border-top: solid 1px gray;
	border-left: solid 1px gray;
	border-right: solid 2px gray;
	border-bottom: solid 2px gray;
	background: #FFFFFF;
	width: relative;
	font-weight: 800;
	color: #f2b052; /*change*/
}

.tabstyle {
	border-width: 5px 1px 1px 1px;
	border-color: #c06f20; /*change*/
	border-style: solid;
	padding: 0px; /*added by shripal */
}

.legend {
	padding-left: 5px;
	padding-right: 5px;
	font-weight: normal;
	font-family: verdana;
	border-width: 0px 0px 1px 0px;
	border-color: #c06f20; /*change*/
	border-style: solid;
}

.headertable {
	cellspacing: 0;
	cellpadding: 0;
	border: 0;
}

.lheadertable_tr td {
	border-top: 1px groove #ffffff;
	border-bottom: 1px solid #454545;
}

#footer tr {
	background: #ededed url(images/footer-bg-01.gif) bottom repeat-x;
}

.tabtable {
	/*	border-right: solid 1px #ffffff;
	border-bottom: solid 1px #ffffff;
	border-top: solid 1px #6587a9;
