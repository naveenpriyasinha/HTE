}

.mandatory {
	background: #FFFFE9;
}

.tabButton {
	border-top: solid 1px #FFFFFF;
	border-left: solid 1px #FFFFFF;
	border-right: solid 1px gray;
	border-bottom: solid 1px gray;
	background: #eedfcc; /*change*/
	width: 80px;
	font-weight: bold;
	color: #000000;
}

.button {
	border-top: solid 1px #FFFFFF;
	border-left: solid 1px #FFFFFF;
	border-right: solid 1px gray;
	border-bottom: solid 1px gray;
	background: #EFEFEF;
	width: 80px;
