}

.toolwin {
	border: solid 1px #887866; /*change*/
}

.toolwin thead {
	background: #887866; /*change*/
}

.fade_background {
	display: none;
	position: absolute;
	top: 0%;
	left: 0%;
	width: 100%;
	height: 100%;
	background-color: #777777;
	z-index: 1001;
	-moz-opacity: 0.6;
	opacity: .60;
	filter: alpha(opacity = 60);
}

/* styles for fieldset and the legend inside it will appear as the header */
.fset {
	width: 100%;
	background: #f2f2f2 url(images/fieldsettop.png) repeat-x top;
	border: none;
	border-bottom: 2px ridge #fff;
	border-left: 1px solid #aaa;
	border-right: 1px ridge #fff;
	padding: 0 1 1 2;
	margin: 0 0;
}

.fset legend {
	width: 100%;
	font-size: 12px;
	font-weight: bold;
	position: relative;
	top: 0px;
	left: 0px;
	padding: 3px 1px 1px 1px;
	margin: 0 0 5 0;
	color: black;
}

