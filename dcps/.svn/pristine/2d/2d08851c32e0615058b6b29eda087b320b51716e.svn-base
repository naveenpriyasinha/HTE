PPROC.POINT4=4. \u0938\u0947\u0935\u093e \u092a\u0941\u0938\u094d\u0924\u200d\u0915 (\u0938\u0947\u0935\u093e\u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u091a\u093e \u0926\u093f\u0928\u093e\u0902\u0915 \u0938\u0947\u0935\u093e \u092a\u0941\u0938\u094d\u0924\u0915\u093e\u0924 \u0928\u092e\u0942\u0926 \u0915\u0930\u093e\u0935\u093e).
PPROC.POINT5A=5(\u090f) \u0930\u093e\u091c\u092a\u0924\u094d\u0930\u093f\u0924 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0928\u0947 \u092f\u0925\u094b\u091a\u093f\u0924\u0930\u093f\u0924\u094d\u092f\u093e \u0938\u093e\u0915\u094d\u0937\u093e\u0902\u0915\u093f\u0924 \u0915\u0947\u0932\u0947\u0932\u094d\u092f\u093e \u0926\u094b\u0928 \u0928\u092e\u0941\u0928\u093e \u0938\u0939\u094d\u0930\u092f\u093e \u0915\u093f\u0902\u0935\u093e \u0938\u094d\u0935\u0924:\u091a\u0940 \u0938\u0939\u0940 \u0915\u0930\u0923\u094d\u092f\u093e\u0907\u0924\u092a\u0924 \u0938\u093e\u0915\u094d\u0937\u0930    \u0928\u0938\u0932\u0947\u0932\u094d\u092f\u093e \u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u0935\u0947\u0924\u0928\u0927\u093e\u0930\u0915\u093e\u091a\u094d\u092f\u093e \u0921\u093e\u0935\u094d\u092f\u093e \u0939\u093e\u0924\u093e\u091a\u093e \u0905\u0902\u0917\u0920\u093e \u0935 \u092c\u094b\u091f\u0947 \u092f\u093e\u0902\u091a\u0947 \u0920\u0938\u0947 \u0905\u0938\u0932\u0947\u0932\u094d\u092f\u093e \u0906\u0923\u093f \u0930\u093e\u091c\u092a\u0924\u094d\u0930\u093f\u0924 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0928\u0947 \u092f\u0925\u094b\u091a\u093f\u0924\u0930\u093f\u0924\u094d\u092f\u093e \u0938\u093e\u0915\u094d\u0937\u093e\u0902\u0915\u093f\u0924 \u0915\u0947\u0932\u0947\u0932\u094d\u092f\u093e \u0926\u094b\u0928 \u091a\u093f\u0920\u094d\u0920\u092f\u093e.
PPROC.POINT5B=(\u092c\u0940) \u092a\u0924\u094d\u0928\u0940\u0938\u0939 \u0915\u093f\u0902\u0935\u093e \u092a\u0924\u0940\u0938\u0939 (\u090f\u0915\u0924\u094d\u0930 \u0915\u093f\u0902\u0935\u093e \u0935\u0947\u0917\u0935\u0947\u0917\u0933\u094d\u092f\u093e) \u0905\u0938\u0932\u0947\u0932\u094d\u092f\u093e \u091b\u093e\u092f\u093e\u091a\u093f\u0924\u094d\u0930\u093e\u091a\u094d\u092f\u093e \u092a\u093e\u0930\u092a\u0924\u094d\u0930 \u0906\u0915\u093e\u0930\u093e\u091a\u094d\u092f\u093e \u2021 \u0924\u0940\u0928 \u092a\u094d\u0930\u0924\u0940. \u0924\u094d\u092f\u093e \u0915\u093e\u0930\u094d\u092f\u093e\u0932\u092f \u092a\u094d\u0930\u092e\u0941\u0916\u093e\u0902\u0928\u0940 \u092f\u0925\u094b\u091a\u093f\u0924\u0930\u093f\u0924\u094d\u092f\u093e \u0938\u093e\u0915\u094d\u0937\u093e\u0902\u0915\u093f\u0924 \u0915\u0947\u0932\u0947\u0932\u094d\u092f\u093e \u0905\u0938\u093e\u0935\u094d\u092f\u093e\u0924.
PPROC.POINT5C=(\u0938\u0940) \u0909\u0902\u091a\u0940 \u0935 \u0913\u0933\u0916\u091a\u093f\u0928\u094d\u0939\u093e\u0902\u091a\u093e \u0924\u092a\u0936\u0940\u0932 \u0926\u0930\u094d\u0936\u0935\u093f\u0923\u093e\u0931\u094d\u092f\u093e, \u0930\u093e\u091c\u092a\u0924\u094d\u0930\u093f\u0924 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0928\u0947 \u092f\u0925\u094b\u091a\u093f\u0924\u0930\u093f\u0924\u094d\u092f\u093e \u0938\u093e\u0915\u094d\u0937\u093e\u0902\u0915\u093f\u0924 \u0915\u0947\u0932\u0947\u0932\u094d\u092f\u093e \u0926\u094b\u0928 \u091a\u093f\u0920\u094d\u0920\u092f\u093e. 
PPROC.POINT6=6. \u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u0935\u0947\u0924\u0928\u093e\u0938\u0902\u092c\u0902\u0927\u0940\u091a\u0940 \u0915\u093e\u0917\u0926\u092a\u0924\u094d\u0930\u0947, \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u091a\u094d\u092f\u093e \u0938\u0947\u0935\u093e\u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u092a\u0942\u0930\u094d\u0935\u0940 \u0938\u0939\u093e \u092e\u0939\u093f\u0928\u094d\u092f\u093e\u0902\u091a\u094d\u092f\u093e \u0906\u0924 \u092a\u093e\u0920\u0935\u093f\u0923\u094d\u092f\u093e\u0924 \u0906\u0932\u0940 \u0928\u0938\u0932\u094d\u092f\u093e\u0938 \u0935\u093f\u0932\u0902\u092c\u093e\u091a\u094d\u092f\u093e \u0915\u093e\u0930\u0923\u093e\u0938\u0902\u092c\u0902\u0927\u0940\u091a\u0947 \u0928\u093f\u0935\u0947\u0926\u0928.
PPROC.POINT7=7. \u092e\u0939\u093e\u0930\u093e\u0937\u094d\u091f\u094d\u0930 \u0928\u093e\u0917\u0930\u0940 \u0938\u0947\u0935\u093e (\u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u0935\u0947\u0924\u0928) \u0928\u093f\u092f\u092e, 1982 \u092e\u0927\u0940\u0932 \u0928\u093f\u092f\u092e 121 (1) (\u090f) (\u091a\u093e\u0930) \u0905\u0928\u0941\u0938\u093e\u0930 \u0906\u0935\u0936\u094d\u092f\u0915 \u0905\u0938\u0947 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u091a\u0947 \u0915\u094b\u0923\u0924\u0947\u0939\u0940 \u0905\u0938\u0932\u094d\u092f\u093e\u0938 \u0932\u0947\u0916\u0940 \u0928\u093f\u0935\u0947\u0926\u0928.
PPROC.POINT8=8. \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0932\u093e \u0938\u0947\u0935\u0947\u0924\u0942\u0928 \u0928\u093f\u0932\u0902\u092c\u093f\u0924 \u0915\u093f\u0902\u0935\u093e \u0938\u0915\u094d\u0924\u0940\u0928\u0947 \u0928\u093f\u0935\u0943\u0924\u094d\u0924 \u0915\u0947\u0932\u094d\u092f\u093e\u0928\u0902\u0924\u0930 \u0915\u093f\u0902\u0935\u093e \u0915\u093e\u0922\u0942\u0928 \u091f\u093e\u0915\u0932\u094d\u092f\u093e\u0928\u0902\u0924\u0930 \u0915\u093f\u0902\u0935\u093e \u092c\u0921\u0924\u0930\u094d\u092b \u0915\u0947\u0932\u094d\u092f\u093e\u0928\u0902\u0924\u0930 \u092a\u0941\u0928\u094d\u0939\u093e \u0915\u093e\u092e\u093e\u0935\u0930 \u0918\u0947\u0924\u0932\u0947 \u0905\u0938\u0932\u094d\u092f\u093e\u0938 \u0924\u094d\u092f\u093e\u0938\u0902\u092c\u0902\u0927\u0940\u091a\u0947 \u0938\u0902\u0915\u094d\u0937\u093f\u092a\u094d\u0924 \u0928\u093f\u0935\u0947\u0926\u0928.
PPROC.T1= \u0935\u093f\u091a\u093e\u0930\u093e\u0924 \u0918\u0947\u0924\u0932\u0947\u0932\u094d\u092f\u093e \u0928\u093f\u0930\u0928\u093f\u0930\u093e\u0933\u094d\u092f\u093e \u0905\u092d\u093f\u0932\u0947\u0916\u093e\u0902\u0924 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0902\u091a\u0940 \u0906\u0926\u094d\u092f\u093e\u0915\u094d\u0937\u0930\u0947 \u0915\u093f\u0902\u0935\u093e \u0928\u093e\u0935 \u092c\u0930\u094b\u092c\u0930 \u0926\u093f\u0932\u0947 \u0928\u0938\u0932\u094d\u092f\u093e\u0938 \u0924\u0940 \u0917\u094b\u0937\u094d\u091f \u092a\u0924\u094d\u0930\u093e\u092e\u0927\u094d\u092f\u0947 \u0928\u092e\u0942\u0926 \u0915\u0930\u093e\u0935\u0940.
PPROC.T2=* \u0906\u0935\u0936\u094d\u092f\u0915 \u0928\u0938\u0932\u0947\u0932\u093e \u092e\u091c\u0915\u0942\u0930 \u0916\u094b\u0921\u093e\u0935\u093e. \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0930\u0940 \u0938\u0947\u0935\u093e\u0928\u093f\u0935\u0943\u0924\u094d\u0924 \u091d\u093e\u0932\u094d\u092f\u093e\u0928\u0902\u0924\u0930 \u091c\u094d\u092f\u093e\u0935\u0947\u0933\u0940 \u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u0935\u0947\u0924\u0928\u093e\u091a\u0947 \u0915\u093e\u0917\u0926\u092a\u0924\u094d\u0930 \u092a\u0921\u0924\u093e\u0933\u0923\u0940\u0938\u093e\u0920\u0940 \u0932\u0947\u0916\u093e\u092a\u0930\u0940\u0915\u094d\u0937\u093e \u0905\u0927\u093f\u0915\u093e\u0931\u094d\u092f\u093e\u0915\u0921\u0947 \u092a\u093e\u0920\u0935\u093f\u0923\u094d\u092f\u093e\u0924 \u092f\u0947\u0924\u093e\u0924 \u0924\u094d\u092f\u093e\u0935\u0947\u0933\u0940 \u092a\u0930\u093f\u091a\u094d\u091b\u0947\u0926 3 \u092e\u0927\u0940\u0932 (\u092c\u0940) \u0939\u0940 \u092c\u093e\u092c \u0932\u093e\u0917\u0942 \u0906\u0939\u0947.
PPROC.T3=\u2020 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0932\u093e \u0938\u0947\u0935\u0947\u0924\u0942\u0928 \u0938\u0915\u094d\u0924\u0940\u0928\u0947 \u0928\u093f\u0935\u0943\u0924\u094d\u0924 \u0915\u0947\u0932\u0947 \u0905\u0938\u0947\u0932 \u0906\u0923\u093f \u0924\u094d\u092f\u093e\u091a\u094d\u092f\u093e\u0915\u0921\u0942\u0928 \u0928\u092e\u0941\u0928\u093e 5 \u092e\u093f\u0933\u0935\u093f\u0923\u094d\u092f\u093e\u0938 \u0935\u093f\u0932\u0902\u092c \u0939\u094b\u0908\u0932 \u0905\u0938\u0947 \u0935\u093e\u091f\u0924 \u0905\u0938\u0947\u0932 \u0924\u0930, \u0915\u093e\u0930\u094d\u092f\u093e\u0932\u092f \u092a\u094d\u0930\u092e\u0941\u0916\u093e\u0902\u0928\u0940 \u0928\u092e\u0941\u0928\u093e 5 \u0935\u094d\u092f\u0924\u093f\u0930\u093f\u0915\u094d\u0924 \u0907\u0924\u0930 \u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u0935\u0947\u0924\u0928\u093e\u0938\u0902\u092c\u0902\u0927\u0940\u091a\u0947 \u0915\u093e\u0917\u0926\u092a\u0924\u094d\u0930 \u0932\u0947\u0916\u093e\u092a\u0930\u0940\u0915\u094d\u0937\u093e \u0905\u0927\u093f\u0915\u093e\u0931\u094d\u092f\u093e\u0915\u0921\u0947 \u092a\u093e\u0920\u0935\u093e\u0935\u0947\u0924 \u0935 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0915\u0921\u0942\u0928 \u0928\u092e\u0941\u0928\u093e 5 \u092e\u093f\u0933\u0924\u093e\u091a \u0924\u094b \u0928\u0902\u0924\u0930 \u092a\u093e\u0920\u0935\u093e\u0935\u093e.
PPROC.T4=\u2021 \u0916\u093e\u0932\u0940\u0932 \u092c\u093e\u092c\u0924\u0940\u0924 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0928\u0947 \u092a\u093e\u0930\u092a\u0924\u094d\u0930 \u0906\u0915\u093e\u0930\u093e\u091a\u094d\u092f\u093e \u091b\u093e\u092f\u093e\u091a\u093f\u0924\u094d\u0930\u093e\u0902\u091a\u094d\u092f\u093e \u092b\u0915\u094d\u0924 \u0926\u094b\u0928 \u092a\u094d\u0930\u0924\u0940 \u092a\u093e\u0920\u0935\u093f\u0923\u0947 \u0906\u0935\u0936\u094d\u092f\u0915 \u0906\u0939\u0947:-
PPROC.T5=(1) \u091c\u0930 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0938, \u092e\u0939\u093e\u0930\u093e\u0937\u094d\u091f\u094d\u0930 \u0928\u093e\u0917\u0930\u0940 \u0938\u0947\u0935\u093e (\u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u0935\u0947\u0924\u0928) \u0928\u093f\u092f\u092e, 1982 \u092e\u0927\u0940\u0932 \u0928\u093f\u092f\u092e 116 \u0932\u093e\u0917\u0942 \u0939\u094b\u0924 \u0905\u0938\u0947\u0932 \u0906\u0923\u093f \u0905\u0935\u093f\u0935\u093e\u0939\u0940\u0924 \u0915\u093f\u0902\u0935\u093e \u0935\u093f\u0927\u0941\u0930 \u0915\u093f\u0902\u0935\u093e \u0935\u093f\u0927\u0935\u093e \u0905\u0938\u0947\u0932 \u0924\u0930,
PPROC.T6=(\u0926\u094b\u0928) \u091c\u0930 \u0936\u093e\u0938\u0915\u0940\u092f \u0915\u0930\u094d\u092e\u091a\u093e\u0931\u094d\u092f\u093e\u0938, \u092e\u0939\u093e\u0930\u093e\u0937\u094d\u091f\u094d\u0930 \u0928\u093e\u0917\u0930\u0940 \u0938\u0947\u0935\u093e (\u0928\u093f\u0935\u0943\u0924\u094d\u0924\u093f\u0935\u0947\u0924\u0928) \u0928\u093f\u092f\u092e, 1982 \u092e\u0927\u0940\u0932 \u0928\u093f\u092f\u092e 117 \u0932\u093e\u0917\u0942 \u0939\u094b\u0924 \u0905\u0938\u0947\u0932 \u0924\u0930.






