<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tcs.sgv.dcps.valueobject">
	<class name="com.tcs.sgv.gpf.valueobject.MstGpfYearly" table="MST_GPF_YEARLY">
		<id name="mstGpfYearlyId" type="long">
			<column name="MST_GPF_YEARLY_ID" />
			<generator class="assigned" />
		</id>
		<property name="gpfAccNo" type="string">
			<column name="GPF_ACC_NO" />
		</property>

		<property name="finYearId" type="long">
			<column name="FIN_YEAR_ID" />
		</property>
		<property name="openingBalance" type="double">
			<column name="OPENING_BALANCE" />
		</property>
		<property name="regularSubscription" type="double">
			<column name="REGULAR_SUBSCRIPTION" />
		</property>
		<property name="advanceRecovery" type="double">
			<column name="ADVANCE_RECOVERY" />
		</property>
		<property name="advanceSanctioned" type="double">
			<column name="ADVANCE_SANCTIONED" />
		</property>
		<property name="withdrawalSanctioned" type="double">
			<column name="WITHDRAWAL_SANCTIONED" />
		</property>
		<property name="excessPayment" type="double">
			<column name="EXCESS_PAYMENT" />
		</property>
		<property name="prePayOfAdvance" type="double">
			<column name="PRE_PAY_OF_ADVANCE" />
		</property>
		<property name="deputationChallan" type="double">
			<column name="DEPUTATION_CHALLAN" />
		</property>
		<property name="closingBalance" type="double">
			<column name="CLOSING_BALANCE" />
		</property>
		<property name="flatInterest" type="double">
			<column name="FLAT_INTEREST" />
		</property>
		<property name="compoundInterest" type="double">
			<column name="COMPOUND_INTEREST" />
		</property>

		<property name="createdDate" type="timestamp">
			<column name="CREATED_DATE" />
		</property>
		<property name="createdUserId" type="long">
			<column name="CREATED_USER_ID" />
		</property>
		<property name="createdPostId" type="long">
			<column name="CREATED_POST_ID" />
		</property>
		<property name="updatedDate" type="timestamp">
			<column name="UPDATED_DATE" not-null="false" />
		</property>
		<property name="updatedUserId" type="long">
			<column name="UPDATED_USER_ID" not-null="false" />
		</property>
		<property name="updatedPostId" type="long">
			<column name="UPDATED_POST_ID" not-null="false" />
		</property>

	</class>
</hibernate-mapping>