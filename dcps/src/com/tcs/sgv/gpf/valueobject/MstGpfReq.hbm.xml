<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tcs.sgv.gpf.valueobject">
	<class name="com.tcs.sgv.gpf.valueobject.MstGpfReq" table="MST_GPF_REQ">
		<id name="mstGpfReqId" type="long">
			<column name="MST_GPF_REQ_ID" />
			<generator class="assigned" />
		</id>
		<property name="transactionId" type="string">
			<column name="TRANSACTION_ID" />
		</property>
		<property name="reqDtlId" type="long">
			<column name="REQ_DTL_ID" />
		</property>
		<property name="reqType" type="string">
			<column name="REQ_TYPE" />
		</property>
		<property name="createdDate" type="timestamp">
			<column name="CREATED_DATE" />
		</property>
		<property name="createdUserId" type="long">
			<column name="CREATED_USER_ID" />
		</property>
		<property name="createdPostId" type="long">
			<column name="CREATED_POST_ID" />
		</property>
		<property name="updatedDate" type="timestamp">
			<column name="UPDATED_DATE" not-null="false" />
		</property>
		<property name="updatedUserId" type="long">
			<column name="UPDATED_USER_ID" not-null="false" />
		</property>
		<property name="updatedPostId" type="long">
			<column name="UPDATED_POST_ID" not-null="false" />
		</property>
		<property name="orderNo" type="string">
			<column name="ORDER_NO" />
		</property>

	</class>
</hibernate-mapping>