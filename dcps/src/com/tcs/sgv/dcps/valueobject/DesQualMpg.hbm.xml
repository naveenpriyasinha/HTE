<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tcs.sgv.dcps.valueobject">
  <class name="DesgQualMpg" table="DESIG_QUALIFICATION_MPG">
    <id name="desQualMpgId" type="long">
            <column name="DES_QUAL_ID" />
            <generator class="assigned" />
        </id>
         <property name="desgName" type="String">
            <column name="date_of_releving" length="20">
                <comment></comment>
            </column>
        </property>
         <property name="qualiName" type="String">
            <column name="qualiName" length="20">
                <comment></comment>
            </column>
        </property>
  </class>
</hibernate-mapping>