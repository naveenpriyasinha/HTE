<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tcs.sgv.dcps.valueobject.DARate" table="MST_DCPS_DA_RATE">

		<id name="daRateIdPk" type="long">
			<column name="DCPS_DA_RATE_MST_ID" precision="20" scale="0" />
			<generator class="assigned" />
		</id>

		<property name="payCommission" type="string">
			<column name="PAY_COMMISSION" />
		</property>
		<property name="daRate" type="float">
			<column name="DA_RATE" />
		</property>
		<property name="effectiveFromDate" type="timestamp">
			<column name="EFFECTIVE_FROM" />
		</property>
		<property name="applicableToDate" type="timestamp">
			<column name="APPLICABLE_TO" />
		</property>
		<property name="status" type="int">
			<column name="status" />
		</property>
		<property name="PostId" type="long">
			<column name="CREATED_POST_ID" />
		</property>
		<property name="UserId" type="long">
			<column name="CREATED_USER_ID" />
		</property>
		<property name="CreatedDate" type="timestamp">
			<column name="CREATED_DATE" />
		</property>

		<property name="UpdatedPostId" type="long">
			<column name="UPDATED_POST_ID" />
		</property>
		<property name="UpdatedUserId" type="long">
			<column name="UPDATED_USER_ID" />
		</property>
		<property name="UpdatedDate" type="timestamp">
			<column name="UPDATED_DATE" />
		</property>
	
	</class>
</hibernate-mapping>
