package com.tcs.sgv.exprcpt.valueobject;

import java.util.Date;

public class BankScrollVO implements java.io.Serializable
{
	private long  bsDetailsId;
	private  String bankName; 
	private Date bsDate;
	private  String bsType;
	private Long attachmentId;
	private Long attachmentSrNo;
	
	public long  getbsDetailsId() {
		return bsDetailsId;
	}
	public void setBsDetailsId(long  bsDetailsId) {
		this.bsDetailsId = bsDetailsId;
	}
	public String getBankName() {
		return bankName;
	}
	public void setBankName(String bankName) {
		this.bankName = bankName;
	}
	public Date getBsDate() {
		return bsDate;
	}
	public void setBsDate(Date bsDate) {
		this.bsDate = bsDate;
	}
	public String getBsType() {
		return bsType;
	}
	public void setBsType(String bsType) {
		this.bsType = bsType;
	}
	public Long getAttachmentId() {
		return attachmentId;
	}
	public void setAttachmentId(Long attachmentId) {
		this.attachmentId = attachmentId;
	}
	public Long getAttachmentSrNo() {
		return attachmentSrNo;
	}
	public void setAttachmentSrNo(Long attachmentSrNo) {
		this.attachmentSrNo = attachmentSrNo;
	}
	
	
	
}
