		this.mstPensionerHdrId = mstPensionerHdrId;
		this.ppoDate = ppoDate;
		this.typeFlag = typeFlag;
		this.currentPostId = currentPostId;
		this.sanctAuthority = sanctAuthority;
	}
	
	public void setPpoNo(String ppoNo) 
	{
		this.ppoNo = ppoNo;
	}

	public String getPpoNo()
	{
		return this.ppoNo;
	}
	public void setCaseSentDate(Date caseSentDate)
	{
		this.caseSentDate = caseSentDate;
	}
	
	public Date getCaseSentDate()
	{
		return this.caseSentDate;
	}
	
	public void setName(String name)
	{
		this.name = name;
	}
	
