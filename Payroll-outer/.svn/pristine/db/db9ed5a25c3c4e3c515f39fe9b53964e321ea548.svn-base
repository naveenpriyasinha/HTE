		return this.name;
	}
	
	public void setCaseInwdDate(Date caseInwdDate)
	{
		this.caseInwdDate = caseInwdDate;
	}
	
	public Date getCaseInwdDate()
	{
		return this.caseInwdDate;
	}
	public void setCommensionDate(Date commensionDate)
	{
		this.commensionDate = commensionDate;
	}
	public Date getCommensionDate()
	{
		return this.commensionDate;
	}
	public void setPensionerCode(String pensionerCode) 
	{
		this.pensionerCode = pensionerCode;
	}

	public String getPensionerCode()
	{
		return this.pensionerCode;
	}
	public void setLyingName(String lyingName)
	{
		this.lyingName = lyingName;
	}
	public String getLyingName()
	{
		return this.lyingName;
	}

	public void setTrnPensionRqstHdrId(Long trnPensionRqstHdrId)
	{
