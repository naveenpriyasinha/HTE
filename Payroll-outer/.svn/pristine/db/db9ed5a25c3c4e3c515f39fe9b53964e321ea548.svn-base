	public  Long getMstPensionerHdrId()
	{
		return this.mstPensionerHdrId;
	}
	public void setPpoDate(Date ppoDate)
	{
		this.ppoDate = ppoDate;
	}
	public Date getPpoDate()
	{
		return this.ppoDate;
	}
	public String getTypeFlag()
    {
        return this.typeFlag;
